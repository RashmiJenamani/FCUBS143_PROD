 VALUES 
('LOV_ICCF_TAGS',
'LCDPRMNT',
'SELECT amount_tag,description FROM(SELECT DISTINCT amount_tag,description FROM   cstbs_amount_tag WHERE  ((interest_allowed = ''Y'' AND language_code = global.lang AND module = ''LC'' AND amount_tag_type IS NULL) OR (amount_tag_type IS NOT NULL AND interest_allowed = ''Y'' AND       ((substr(amount_tag, 1, instr(amount_tag, ''_'', -1) - 1) IN  (SELECT component FROM cftms_product_iccf WHERE  product = ?))))) AND ''I'' = (SELECT TYPE   FROM   cftms_rule WHERE  rule = ? AND currency = ''ALL'' AND currency2 = ''ALL'' AND customer = ''ALL'' AND customer_group = ''ALL'' AND branch_code = ''ALL'') UNION SELECT DISTINCT amount_tag,description FROM   cstbs_amount_tag WHERE  (commission_allowed = ''Y'' AND language_code = global.lang AND module = ''LC'') AND ''C'' = (SELECT TYPE FROM cftms_rule WHERE  rule = ? AND currency = ''ALL'' AND currency2 = ''ALL'' AND customer = ''ALL'' AND customer_group = ''ALL'' AND                     branch_code = ''ALL'')UNION SELECT DISTINCT amount_tag,description FROM   cstbs_amount_tag WHERE  ((charge_allowed = ''Y'' AND language_code = global.lang AND module = ''LC'' AND amount_tag_type IS NULL) OR       (amount_tag_type IS NOT NULL AND charge_allowed = ''Y'' AND       ((substr(amount_tag, 1, instr(amount_tag, ''_'', -1) - 1) IN    (SELECT component  FROM cftms_product_iccf  WHERE  product = ?))))) AND ''H'' = (SELECT TYPE FROM   cftms_rule  WHERE  rule = ? AND currency = ''ALL'' AND currency2 = ''ALL'' AND customer = ''ALL'' AND customer_group = ''ALL'' AND branch_code = ''ALL'')) WHERE 1=1 ORDER BY amount_tag',
100,'AMOUNT_TAG!TEXT!AMOUNT_TAG~DESCRIPTION!TEXT!DESCRIPTION',
'STRING~STRING',
'','','')
;
END;
