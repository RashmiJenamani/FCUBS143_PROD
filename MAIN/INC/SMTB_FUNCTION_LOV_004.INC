'select cust_no from ( SELECT DISTINCT CUST_NO FROM STTM_CUST_TAX_LIMIT WHERE TAX_GROUP IN ( select   tax_group from   sttm_customer where   customer_no=( select   cust_no from sttm_cust_account where    cust_ac_no=? and     branch_code=? and     record_stat=''O'' and auth_stat=''A'' ) and     record_stat=''O'' and auth_stat=''A'' UNION select   tax_group from    sttm_customer where    customer_no=(select counterparty from cstb_contract where contract_ref_no=? ) and     record_stat=''O'' and auth_stat=''A'' ) AND TAX_CATEGORY=? AND FROM_DATE <=? AND  NVL(TO_DATE,''31-DEC-4712'') >= NVL(?,?) AND CUST_NO <> ''ALL'' and record_stat=''O'' and auth_stat=''A'' UNION SELECT DISTINCT CUST.CUSTOMER_NO CUST_NO FROM STTM_CUSTOMER CUST, STTM_CUST_ACCOUNT ACC WHERE ACC.CUST_NO = CUST.CUSTOMER_NO AND CUST.TAX_GROUP IN ( select   tax_group from   sttm_customer where   customer_no=( select   cust_no from sttm_cust_account where    cust_ac_no=? and     branch_code=? and     record_stat=''O'' and auth_stat=''A'' ) and record_stat=''O'' and auth_stat=''A'' UNION select  tax_group from   sttm_customer where   customer_no=(select counterparty from cstb_contract where contract_ref_no=? ) and     record_stat=''O'' and auth_stat=''A'' ) AND EXISTS(SELECT 1 FROM STTM_CUST_TAX_LIMIT WHERE TAX_GROUP IN ( select   tax_group from   sttm_customer where   customer_no=( select   cust_no from sttm_cust_account where    cust_ac_no=? and     branch_code=? and     record_stat=''O'' and auth_stat=''A'' ) and     record_stat=''O'' and auth_stat=''A'' UNION select  tax_group from   sttm_customer where   customer_no=(select counterparty from cstb_contract where contract_ref_no=? ) and     record_stat=''O'' and auth_stat=''A'' ) AND TAX_CATEGORY=? AND FROM_DATE <=? AND NVL(TO_DATE,''31-DEC-4712'') >= NVL(?,?) AND CUST_NO=''ALL'' and record_stat=''O'' and auth_stat=''A'') MINUS (SELECT DISTINCT CUST_NO FROM CSTM_TAX_LIMIT_DETAIL WHERE TAX_CATEGORY=? AND BRANCH_CODE=? AND ACC_REF_NO=? AND START_DATE=?) ) wher'||
