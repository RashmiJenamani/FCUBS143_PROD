                    ''MODIFY''' || chr(10) || '                                                  ELSE' || chr(10) || '                                                   Substr(?, Instr(?, ''_'', -1) + 1)' || chr(10) || '                                               END AS Action' || chr(10) || '                                       FROM   Smtb_Menu a' || chr(10) || '                                       WHERE  Function_Id = c.Call_Form)' || chr(10) || '                     '||
'END AS Action' || chr(10) || '                          FROM   Cstb_Fid_Callforms c' || chr(10) || '                          WHERE  Function_Id IN (SELECT Fc_Function_Id' || chr(10) || '                                                 FROM   Gwtm_Operations_Master' || chr(10) || '                                                 WHERE  Service_Name = ?' || chr(10) || '                                                 AND    Operation_Code = ?' || chr(10) || '                                                 UNION' || chr(10) || '                                                 SELECT Service_Name' || chr(10) || '          FROM   Gwtb_Amend_Nodes' || chr(10) || '                                                 WHERE  Service_Name = ?' || chr(10) || '                                                 AND    Operation_Code = ?)))' || chr(10) || ') WHERE 1=1' || chr(10) || 'order by field_name',
