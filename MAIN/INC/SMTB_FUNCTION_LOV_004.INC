 VALUES 
('LOV_EXPO_LINE',
'SEDSKLOC',
'SELECT LINE_CD FROM(SELECT LINE_CD||LINE_SERIAL LINE_CD FROM ELVW_LIMITS_INT l WHERE l.liab_id=(SELECT cust1.liability_no FROM sttms_customer cust1 WHERE cust1.customer_no=?)AND nvl(l.last_new_util_date,to_date(''99991231'',''YYYYMMDD''))>=(SELECT dates.today FROM sttms_dates dates WHERE dates.branch_code=GLOBAL.current_branch) AND l.record_stat=''O'' AND l.auth_stat=''A'' and l.availability_flag=''Y'' AND(l.line_currency= GLOBAL.LCY OR l.ccy_restriction=''N'')AND(EXISTS(SELECT * FROM ELVW_LIMITS_CUST_REST_INT cr  WHERE cr.liab_id=(SELECT cust2.liability_no FROM sttms_customer cust2 WHERE cust2.customer_no=?)AND cr.line_cd=l.line_cd AND cr.line_serial=l.line_serial AND cr.customer_no=?)OR NOT EXISTS(SELECT * FROM ELVW_LIMITS_CUST_REST_INT cr  WHERE cr.liab_id=(SELECT cust3.liability_no FROM sttms_customer cust3 WHERE cust3.customer_no=?)AND cr.line_cd=l.line_cd AND cr.line_serial=l.line_serial))AND(EXISTS(SELECT * FROM ELVW_LIMITS_PROD_REST_INT pr WHERE pr.liab_id=(SELECT cust4.liability_no FROM sttms_customer cust4 WHERE cust4.customer_no=?)AND pr.line_cd=l.line_cd AND pr.line_serial=l.line_serial AND pr.product_code=''ZSKR'')OR NOT EXISTS(SELECT * FROM ELVW_LIMITS_PROD_REST_INT pr1 WHERE pr1.liab_id=(SELECT cust5.liability_no FROM sttms_customer cust5 WHERE cust5.customer_no=?)AND pr1.line_cd=l.line_cd AND pr1.line_serial=l.line_serial))AND (EXISTS(SELECT * FROM ELVW_LIMITS_BR_REST_INT b WHERE b.liab_id=(SELECT cust6.liability_no FROM sttms_customer cust6 WHERE cust6.customer_no=?)AND b.line_cd=l.line_cd AND b.line_serial=l.line_serial AND b.branch_code=GLOBAL.current_branch)OR NOT EXISTS(SELECT * FROM ELVW_LIMITS_BR_REST_INT b1 WHERE b1.liab_id=(SELECT cust7.liability_no FROM sttms_customer cust7 WHERE cust7.customer_no=?)AND b1.line_cd=l.line_cd AND b1.line_serial=l.line_serial))AND(EXISTS(SELECT * FROM ELVW_LIMITS_CCY_REST_INT cy WHERE cy.liab_id=(SELECT cust8.liability_no FROM sttms_customer cust8 WHERE cust8.customer_no=?)AND cy.line_cd=l.line_cd AND cy.line_'||
'serial=l.line_serial AND cy.ccy_code= GLOBAL.LCY) OR NOT EXISTS (SELECT * FROM ELVW_LIMITS_CCY_REST_INT cy1 WHERE cy1.liab_id=(SELECT cust9.liability_no FROM sttms_customer cust9 WHERE cust9.customer_no=?)AND cy1.line_cd=l.line_cd AND cy1.line_serial=l.line_serial))) WHERE 1=1',
1000,'LINE_CD!TEXT!LINE_CD',
'STRING',
