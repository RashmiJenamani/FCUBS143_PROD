'select function_id, description from (SELECT FUNCTION_ID, DESCRIPTION FROM (SELECT A.FUNCTION_ID, MAIN_MENU || chr(9) || SUB_MENU_1 || chr(9) || SUB_MENU_2 DESCRIPTION FROM SMTB_FUNCTION_DESCRIPTION A, SMTB_MENU B WHERE A.Lang_Code = global.lang AND A.FUNCTION_ID = B.FUNCTION_ID AND ((B.TYPE_STRING IN (''M'', ''ELM'') AND SUBSTR(A.FUNCTION_ID, 3, 1) NOT IN (''S'', ''P'') AND A.MAIN_MENU || A.SUB_MENU_1 || A.SUB_MENU_2 IS NOT NULL)) )) WHERE 1 = 1 ORDER BY FUNCTION_ID',
'Y~Y~',
'VARCHAR2~VARCHAR2~',
'')
;
END;
